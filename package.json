{
  "name": "vitepress-plugin-twoslash",
  "description": "VitePress plugin for twoslash",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/wagmi-dev/vitepress-plugin-twoslash.git"
  },
  "author": "awkweb.eth",
  "ethereum": "wagmi-dev.eth",
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/wagmi-dev"
    }
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.mts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/dist"
  ],
  "peerDependencies": {
    "vitepress": "*"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.0",
    "@types/eslint": "^8.4.10",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "17.0.21",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "bun": "^0.5.7",
    "eslint": "8.17.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^11.1.0",
    "lint-staged": "^12.4.1",
    "prettier": "^2.8.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "vitepress",
    "shiki",
    "twoslash"
  ],
  "scripts": {
    "build": "tsup",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "dev": "DEV=true tsup",
    "docs:dev": "pnpm -r --filter docs dev",
    "docs:build": "pnpm -r --filter docs build",
    "docs:preview": "pnpm -r --filter docs preview",
    "lint": "eslint . --cache",
    "lint:fix": "pnpm lint --fix",
    "lint:format": "prettier --write",
    "postinstall": "pnpm dev",
    "prepare": "npx simple-git-hooks",
    "prepublishOnly": "pnpm bun scripts/prepublishOnly.ts",
    "typecheck": "tsc --noEmit"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint:fix"
    ],
    "*.{json,md,mdx,yml}": [
      "pnpm lint:format"
    ]
  },
  "packageManager": "pnpm@7.5.0",
  "pnpm": {
    "overrides": {
      "remark-shiki-twoslash>shiki": "^0.14.1",
      "shiki-twoslash>shiki": "^0.14.1"
    },
    "patchedDependencies": {
      "vitepress@1.0.0-alpha.48": "patches/vitepress@1.0.0-alpha.48.patch",
      "shiki-twoslash@3.1.1": "patches/shiki-twoslash@3.1.1.patch"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "engines": {
    "pnpm": ">=7"
  }
}
